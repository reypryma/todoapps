<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootAnchorpane" prefHeight="80.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.CellController">
   <children>
      <ImageView fx:id="iconImageView" fitHeight="56.0" fitWidth="61.0" layoutX="4.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../asset/write_sign-01.png" />
         </image>
      </ImageView>
      <Label fx:id="taskLabel" layoutX="55.0" layoutY="12.0" prefHeight="24.0" prefWidth="91.0" text="Task">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="descriptionLabel" layoutX="57.0" layoutY="36.0" prefHeight="18.0" prefWidth="241.0" style="-fx-background-color: #2ecc71;" text="Description" textFill="WHITE" />
      <Label fx:id="dateLabel" layoutX="212.0" layoutY="58.0" prefHeight="18.0" prefWidth="77.0" text="Date Created" />
      <ImageView fx:id="deleteButton" fitHeight="18.0" fitWidth="21.0" layoutX="268.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../asset/trash.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
